<!DOCTYPE gsa-template PUBLIC "-//Art Technology Group, Inc.//DTD Dynamo Security//EN" 
"http://www.atg.com/dtds/gsa/gsa_1.0.dtd">
<gsa-template>
  <header>
    <name>Lock Repository</name>
    <author>Vyachaslau Koshman</author>    
  </header>  
  <item-descriptor name="lock" display-property="id">
      <table name="lock" type="primary"  id-column-name="id">
      <property name="id"  data-type="string"/>  
      <property name="isLocked" column-name="isLocked" data-type="boolean"/>   
               
    </table>     
  </item-descriptor>
</gsa-template>